<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.actions.act_window" id="action_cfg_sms">
			<field name="name">短信通道</field>
			<field name="res_model">ex.cfg.sms</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p baby="oe_view_nocontent_create">创建第一条数据
				</p>
			</field>
		</record>

		<record model="ir.ui.view" id="view_cfg_sms_form">
			<field name="name">短信通道</field>
			<field name="model">ex.cfg.sms</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form >
					<sheet>
						<group>
							<field name="name" readonly="1"/>

						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="view_cfg_sms_tree">
			<field name="name">短信通道</field>
			<field name="model">ex.cfg.sms</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" readonly="1"/>


				</tree>
			</field>
		</record>


		<record model='ir.ui.view' id='view_cfg_sms_search'>
			<field name="name">短信通道</field>
			<field name="model">ex.cfg.sms</field>
			<field name="arch" type="xml">
				<search string="名称" >
					<field name="name" />
				</search>
			</field>
		</record>

	</data>
</openerp>
